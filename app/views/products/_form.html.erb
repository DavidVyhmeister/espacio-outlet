<%= form_with(model: product) do |form| %>


  <div class="field">
    <%= form.label :propietario %>
    <%= form.text_field :propietario, :value => current_user.email,:readonly => true %>
  
  </div>

  <div class="field">
    <%= form.label :nombre %>
    <%= form.text_field :nombre %>
  </div>

  <div class="field">
    <%= form.label :codigo %>
    <%= form.text_field :codigo %>
  </div>

  <div class="field">
    <%= form.label :descripcion %>
    <%= form.text_field :descripcion %>
  </div>

  <div class="field">
    <%= form.label :cantidad %>
    <%= form.number_field :cantidad %>
  </div>



  <div class="actions">
    <%= form.submit %>
  </div>
<% end %>
